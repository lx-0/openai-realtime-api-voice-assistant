modules = ["nodejs-20", "nix"]
hidden = [".config", "package-lock.json", "tsconfig.json"]
run = "tsx index.ts"
entrypoint = "index.ts"

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_05"

[unitTest]
language = "nodejs"

[deployment]
run = ["tsx", "index.ts"]
deploymentTarget = "cloudrun"
ignorePorts = false

[[ports]]
localPort = 5050
externalPort = 80
exposeLocalhost = true
